<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">

    <Button
        android:id="@+id/main_jump_to_First"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="携带参数跳转"/>

    <Button
        android:id="@+id/main_jump_to_Sencond"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="返回结果值"/>

    <Button
        android:id="@+id/main_jump_to_third"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="加载Fragment"/>

    <Button
        android:id="@+id/main_get_data_from_webview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="加载webview页面"/>

    <Button
        android:id="@+id/id_implicit_native"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="隐式唤起app"
        />

    <Button
        android:id="@+id/id_implicit_page"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="隐式唤醒activity"/>

    <Button
        android:id="@+id/id_module1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="跳转到模块2"/>

</LinearLayout>
