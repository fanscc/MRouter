<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.wuba.mobile.middle.app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.wuba.mobile.middle.app.FirstActvity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan">
        </activity>

        <activity
            android:name="com.wuba.mobile.middle.app.SecondActvity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan">
        </activity>

        <activity
            android:name="com.wuba.mobile.middle.app.ThirdActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan">
        </activity>

        <activity android:name=".SchemeFilterActivity">

            <intent-filter>
                <action android:name="android.intent.action.VIEW"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
                <category android:name="android.intent.category.BROWSABLE"></category>

                <data
                    android:host="mis.android"
                    android:scheme="router"></data>
            </intent-filter>
        </activity>

        <activity android:name=".WebViewActivity"></activity>
        <activity android:name=".TargetActivity"></activity>

        <activity android:name=".ImplicitActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
                <data
                    android:host="implicit"
                    android:scheme="router"
                    ></data>
            </intent-filter>
        </activity>
    </application>
</manifest>
